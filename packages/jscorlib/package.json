{
  "name": "jscorlib",
  "version": "0.0.0-int.1",
  "description": "a set of JS helpers making .NET developers feel more at home.",
  "repository": "https://github.com/CXuesong/jscorlib.git",
  "author": "CXuesong",
  "license": "MIT",
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pwsh -File ./scripts/build.ps1",
    "build:prod": "pwsh -File ./scripts/build.ps1 -Clean",
    "lint": "eslint src",
    "test": "vitest",
    "benchmark": "vitest bench"
  },
  "devDependencies": {
    "@jscorlib-repo/linters": "workspace:^",
    "@jscorlib-repo/test-helpers": "workspace:^",
    "@jscorlib-repo/typedoc-presets": "workspace:^",
    "typescript": "^5.4.5"
  }
}
