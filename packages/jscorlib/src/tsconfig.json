{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "../lib"
  },
  "include": [
    "*.ts",
  ],
}