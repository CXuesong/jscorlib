{
  "name": "@jscorlib-repo/gh-pages-hosting",
  "version": "0.0.1",
  "description": "building scripts for GitHub Pages",
  "repository": "https://github.com/CXuesong/jscorlib.git",
  "author": "CXuesong",
  "license": "MIT",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "pwsh -File ./scripts/build.ps1",
    "build:collect-resources": "pwsh -File ./scripts/build.ps1 -CollectResources",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./src/tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tsconfig/svelte": "^5.0.4",
    "carbon-components-svelte": "^0.85.2",
    "carbon-icons-svelte": "^12.11.0",
    "sass": "^1.79.3",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
